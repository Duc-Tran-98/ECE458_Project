{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "react/jsx-filename-extension": [1, { "extensions": ["js", "jsx"] }],
        "max-len": ["error", { "code": 200 }],
        "jsx-a11y/label-has-associated-control": "off",
        "jsx-a11y/label-has-for":"off"
    }
}
